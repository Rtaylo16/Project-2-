

<table class="table table-hover table-fixed" id="playlist">

  <!--Table head-->
  <thead>
    <tr>
      
      <th>Title</th>
      <th>Artist</th>
      <th>Album</th>
      <th>Add Track</th>
      <th>Delete Track</th>
    </tr>
  </thead>

  <tbody>
    {{#each track}}
    <tr>
        <td>{{name}}</td>
        <td>{{artist}}</td>
        <td>{{album}}</td>
    </tr>
    {{/each}}
  </tbody>

    

</table>



